<%@ page import="cliente.Jogador, tools.User, java.util.ArrayList" language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>OTHELLO GAME</title>
<script>
var click=false;
function doClick (img, simbolo, q) {
	if(!click) {
		img.src=simbolo; 
		document.getElementById('quadricula').value=q; 
		document.getElementById('Jogar').submit();
		document.getElementById('msgJogada').innerHTML="";
		document.getElementById('msg').innerHTML="<h2>Jogou ("+q+"), espera pelo oponente...</h2>";
		click=true;
	}
}
</script>
<%!
private String getSimbolo(String tabuleiro){
	int i = tabuleiro.indexOf(":");
	if(i!=-1)
		return tabuleiro.substring(i-1,i);
	return "?";
}

private boolean terminou(String tabuleiro){
	return tabuleiro.contains("VitÃ³ria do ");
}

private char[][] getTabuleiro(String tabuleiro) {
	char[][] othelloBoard = new char[8][8];
	char row[] = new char[8];
	
	int j = 0;
	int aux = 0;
	for (int i = 1; i < tabuleiro.length(); i++) {
	    if (aux == 8){
	    	break;
	    }
		if (j==8){
			othelloBoard[aux] = row;
			row = new char[8];
			j = 0;
			aux++;
		}
		
	    if (tabuleiro.charAt(i) == 'X' || tabuleiro.charAt(i) == 'O' || (tabuleiro.charAt(i) == ' ' && tabuleiro.charAt(i-1) == '|')) {
	    	row[j] = tabuleiro.charAt(i);
	    	j++;
		}
	}
	return othelloBoard;
}

private String mensagem(String tabuleiro) {
	return "<h2>"+tabuleiro.substring(tabuleiro.length()-7)+"</h2>";
}

private String tabuleiroHTML(String tabuleiro) {
	String simbolo = getSimbolo(tabuleiro);

	final String board = "data:image/jpeg;base64,";
	final String X = "data:image/png;base64,";
	final String O = "data:image/png;base64,";
	final String Q = "data:image/png;base64,";
	
	char[][] tab = getTabuleiro(tabuleiro);
	String table = "<table style=\"background-image: url('"+board+"'); height: 400px; width: 460px; background-repeat: no-repeat; background-size: 100% 100%;\">";
	
	for(int l = 0; l< 8; l++) {
		table = table + "<tr>";
		
		for(int c = 0; c < 8; c++) {
			String img = Q;
			String jogada = (l+1)+" "+(c+1);
			
			if( tab[l][c] == 'O' )
				img = O;
			
			if( tab[l][c] == 'X' )
				img = X;
			   
			table=table+"<td style=\"padding: 3px; vertical-align: center; text-align: center; height: 100px; width: 88px;\">";
			table=table+"<img onclick=\"doClick(this, '"+simbolo+"', '"+jogada+"');\" style=\"height: 85%; width: 85%;\" title=\"QuadrÃ­cula "+jogada+"\" src=\""+img+"\"></img></td>";
			table=table+"</td>";
		}
	    table=table+"</tr>";
	}
	return table+"</table>";
}

%>

<%
